<?xml version="1.0" encoding="UTF-8" ?>
<div xmlns:jsp="http://java.sun.com/JSP/Page"
	 xmlns:c="http://java.sun.com/jsp/jstl/core"
	 xmlns:em="http://e-monocot.org/portal/functions"
	 xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	 xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
	 xmlns:tags="urn:jsptagdir:/WEB-INF/tags"
	 xmlns:geo="urn:jsptagdir:/WEB-INF/tags/geo"
	 xmlns:spring="http://www.springframework.org/tags"
	 xmlns:security="http://www.springframework.org/security/tags"
	 itemscope="itemscope" itemtype="http://rs.tdwg.org/dwc/terms/Taxon" itemid="${taxon.identifier}">
	<jsp:directive.page import="java.util.Date"/>
	<jsp:directive.page import="java.text.SimpleDateFormat"/>
	<spring:message code="distribution.map" var="messageDistribution"/>
	<tags:pageheader>
	<h2 id="page-title">
<!--		<a href="#WFOID" title="World Flora Online ID" rel="tooltip"></a>-->
			<a href= "http://www.worldfloraonline.org/taxon/${taxon.identifier}"><b class="WFOID">${taxon.identifier}</b></a><br></br>

	<tags:feedbackLink selector="option:contains(${taxon.scientificName} ${taxon.scientificNameAuthorship})" dataName="${taxon.scientificName}" />
	<i class="glyphicons-icon leaf"><!--  --></i> <em class="taxonName" itemprop="scientificName">${taxon.scientificName}</em>&#160;<span itemprop="scientificNameAuthorship">${taxon.scientificNameAuthorship}</span>
	<ul class="unstyled">
	<p>
        <li>
            <small class="protologueText">
                <span id="protologue" itemprop="namePublishedIn">${taxon.namePublishedInString}</span>
                <c:if test="${not empty taxon.nomenclaturalStatus}">&#160;<span itemprop="nomenclaturalStatus" content="${taxon.nomenclaturalStatus}"><spring:message code="nomenclatural_status_abbreviation_${taxon.nomenclaturalStatus}" text="${taxon.nomenclaturalStatus}"/></span></c:if>
            </small>
        </li>
    </p>

	<c:if test="${not empty preferred}">
		<li>
			<c:set var="preferred" value="${em:getPreferred(taxon)}" />
			<small>${preferred.vernacularName}</small>
		</li>
	</c:if>
	<p>

		<li>
	<small class="taxonomicStatusText">

		<div id="taxonomicStatus">
			<section>
				<!--	order and majorGroup-->
				<c:if test="${taxon.taxonRank == 'ORDER' || taxon.taxonRank == 'PHYLUM'}">
					<spring:message code="isAcceptedBy" />
					<c:url var="organisationUrl" value="/organisation/${taxon.authority.identifier}" />
					<a href="${organisationUrl}">${taxon.authority.title}</a>
				</c:if>

				<!--	family-->
				<c:if test="${taxon.taxonRank == 'FAMILY'}">
					<c:if test="${not empty taxon.taxonExternalLinks.majorGroup}">
						<c:set var="majorGroupName" value="${em:majorGroupName(taxon.taxonExternalLinks.majorGroup)}" />
						<c:set var="majorGroupwfoid" value="${em:getAncestorsWFOId(taxon,majorGroupName)}" />
						<c:url var="majorGroupUrl" value="/taxon/${majorGroupwfoid}" />
					<p>	The family <i>${taxon.scientificName}</i> is in the major group <a href="${majorGroupUrl}">${majorGroupName}</a>.</p>
					</c:if>
				</c:if>

				<!--	genus-->
				<c:if test="${taxon.taxonRank == 'GENUS'}">
				<c:if test="${not empty taxon.taxonExternalLinks.majorGroup and not empty taxon.family}">
					<c:set var="majorGroupName" value="${em:majorGroupName(taxon.taxonExternalLinks.majorGroup)}" />
					<c:set var="majorGroupwfoid" value="${em:getAncestorsWFOId(taxon,majorGroupName)}" />
					<c:set var="familywfoid" value="${em:getAncestorsWFOId(taxon,taxon.family)}" />
					<c:url var="familyUrl" value="/taxon/${familywfoid}" />
					<c:url var="majorGroupUrl" value="/taxon/${majorGroupwfoid}" />
				<p>
					The genus <i>${taxon.scientificName}</i> is in the family
					<a href="${familyUrl}">${taxon.family}</a> in the major group
					<a href="${majorGroupUrl}">${majorGroupName}</a>
					by <c:url var="organisationUrl" value="/organisation/${taxon.authority.identifier}" />
					<a href="${organisationUrl}">${taxon.authority.title}</a>.
				</p>
				</c:if>
				</c:if>

				<!--//species-->
				<c:if test="${taxon.taxonRank != 'PHYLUM' and taxon.taxonRank != 'ORDER' and taxon.taxonRank != 'FAMILY' and taxon.taxonRank != 'GENUS' }">
					<c:choose>
						<c:when test="${em:isSynonym(taxon) and taxon.acceptedNameUsage != null}">
							<p>
							<spring:message code="isSynonymOf" />
							<c:url var="acceptedUrl" value="/taxon/${taxon.acceptedNameUsage.identifier}" />
							<a href="${acceptedUrl}">${taxon.acceptedNameUsage.scientificName}
								${taxon.acceptedNameUsage.scientificNameAuthorship}
							</a>
							by <c:url var="organisationUrl" value="/organisation/${taxon.authority.identifier}" />
							<a href="${organisationUrl}">${taxon.authority.title}</a>.
							</p>
						</c:when>
						<c:when test="${em:isHeterotypicSynonym(taxon) and taxon.acceptedNameUsage != null}">
							<p>
							<spring:message code="isHeterotypicSynonymOf" />
							<c:url var="acceptedUrl" value="/taxon/${taxon.acceptedNameUsage.identifier}" />
							<a href="${acceptedUrl}">${taxon.acceptedNameUsage.scientificName}
								${taxon.acceptedNameUsage.scientificNameAuthorship}
							</a>
							by <c:url var="organisationUrl" value="/organisation/${taxon.authority.identifier}" />
							<a href="${organisationUrl}">${taxon.authority.title}</a>.
	</p>
						</c:when>
						<c:when test="${em:isHomotypicSynonym(taxon) and taxon.acceptedNameUsage != null}">
							<p>
							<spring:message code="isHomotypicSynonymOf" />
							<c:url var="acceptedUrl" value="/taxon/${taxon.acceptedNameUsage.identifier}" />
							<a href="${acceptedUrl}">${taxon.acceptedNameUsage.scientificName}
								${taxon.acceptedNameUsage.scientificNameAuthorship}
							</a>
							by <c:url var="organisationUrl" value="/organisation/${taxon.authority.identifier}" />
							<a href="${organisationUrl}">${taxon.authority.title}</a>.
						</p>
						</c:when>
						<c:when test="${em:isSynonym(taxon) and taxon.acceptedNameUsage == null}">
							<p>
							<spring:message code="isNotAcceptedBy" />
							<c:url var="organisationUrl" value="/organisation/${taxon.authority.identifier}" />
							<a href="${organisationUrl}">${taxon.authority.title}
							</a>
						</p>
						</c:when>
						<c:when test="${taxon.taxonomicStatus == 'Doubtful'}">
							This name is reported by <c:url var="organisationUrl" value="/organisation/${taxon.authority.identifier}" />
							<a href="${organisationUrl}">${taxon.authority.title}</a> as ambiguous
						</c:when>
						<c:when test="${taxon.taxonomicStatus == 'Unchecked'}">
							<spring:message code="isUnchecked" />
						</c:when>
						<c:when test="${taxon.taxonomicStatus == 'Accepted'}">

							<p>

								<c:if test="${not empty taxon.verbatimTaxonRank and taxon.verbatimTaxonRank != null}">

								<c:if test="${not empty taxon.verbatimTaxonRank and taxon.verbatimTaxonRank != null and taxon.verbatimTaxonRank != 'family' and  taxon.verbatimTaxonRank != 'genus' and taxon.verbatimTaxonRank != 'gen.' and taxon.verbatimTaxonRank != 'order' and taxon.verbatimTaxonRank != 'sp.' and taxon.verbatimTaxonRank != 'Species'}">
									This name is the accepted name of an infraspecific taxon
<!--									<a href="#incompleteInfraspecific">*</a>-->
									<c:if test="${not empty taxon.parentNameUsage}">of the species
										<c:url var="parentUrl" value="/taxon/${taxon.parentNameUsage.identifier}" />
										<a href="${parentUrl}">${taxon.parentNameUsage.scientificName}
											${taxon.parentNameUsage.scientificNameAuthorship}
										</a>
									</c:if>
								</c:if>
								</c:if>

								<c:if test="${empty taxon.verbatimTaxonRank}">

								This name is reported by <c:url var="organisationUrl" value="/organisation/${taxon.authority.identifier}" />
                            <a href="${organisationUrl}">${taxon.authority.title}</a> as an accepted name
								</c:if>

                            in the genus
                            <c:set var="genuswfoid" value="${em:getAncestorsWFOId(taxon,taxon.genus)}" />
                            <c:url var="genusUrl" value="/taxon/${genuswfoid}" />
                            <a href="${genusUrl}">${taxon.genus}</a>

                            <c:set var="familywfoid" value="${em:getAncestorsWFOId(taxon,taxon.family)}" />
                            <c:url var="familyUrl" value="/taxon/${familywfoid}" />
                            (family <a href="${familyUrl}">${taxon.family}</a>).

                        </p>
						</c:when>
					</c:choose>
				</c:if>

				<c:if test="${not empty taxon.source}">

					<c:set var="source" value="${em:getFullSource(taxon.source)}" />
					<p>
						The record derives from <b>${source}</b>

                        <c:if test="${not empty taxon.modified}">

							<fmt:parseDate value="${taxon.modified}" pattern="yyyy-MM-dd" var="date"/>
							(data supplied on <fmt:formatDate value="${date}" pattern="yyyy-MM-dd"/>)
                        </c:if>
						which
						<c:choose>
							<c:when test="${taxon.taxonomicStatus == 'Accepted'}">
								reports it as an accepted
								name</c:when>
							<c:when test="${taxon.taxonomicStatus == 'Misapplied'}">
								reports it as a misapplied
								name</c:when>

							<c:when test="${taxon.taxonomicStatus == 'Synonym'}">reports it as a
								synonym
							</c:when>
							<c:otherwise>reports it as a ${taxon.taxonomicStatus} name</c:otherwise>
						</c:choose>
						<c:choose>
							<c:when test="${taxon.taxonExternalLinks.ccrStatus == 'Illegitimate'}"> and as an <b>illegitimate</b> name</c:when>
							<c:when test="${taxon.taxonExternalLinks.ccrStatus == 'Invalid'}"> and as an <b>invalid</b> name</c:when>
							<c:when test="${taxon.taxonExternalLinks.ccrStatus == 'Orthographic'}"> and as a <b>spelling variant</b></c:when>
						</c:choose>
						<c:if test="${not empty taxon.taxonExternalLinks.localID and taxon.source != 'ksufab' and taxon.source != 'ksu' and taxon.source != 'cmp'}">
							(record <c:choose>
							<c:when test="${taxon.source == 'tro'   }">
								<a rel="external" href="http://www.tropicos.org/Name/${taxon.taxonExternalLinks.localID}">${taxon.taxonExternalLinks.localID}</a>
							</c:when>
							<c:when test="${taxon.source == 'gcc'   }">
								<a rel="external" href="http://compositae.landcareresearch.co.nz/?Page=NameDetails&amp;NameId=${taxon.taxonExternalLinks.localID}">${taxon.taxonExternalLinks.localID}</a>

							</c:when>
							<c:when test="${taxon.source == 'ifern' }">

									<a rel="external" href="http://www.ipni.org/ipni/idPlantNameSearch.do?id=${taxon.taxonExternalLinks.localID}">${taxon.taxonExternalLinks.localID}</a>

							</c:when>
							<c:when test="${taxon.source == 'wcs' || taxon.source == 'iplants' || taxon.source == 'iopi' }">
								<a rel="external" href="http://apps.kew.org/wcsp/namedetail.do?name_id=${taxon.taxonExternalLinks.localID}">${taxon.taxonExternalLinks.localID}</a>
							</c:when>
							<c:when test="${taxon.source == 'ildis' }">
								<a rel="external" href="http://www.ildis.org/LegumeWeb?genus=${taxon.genus}&amp;species=${taxon.specificEpithet}">${taxon.taxonExternalLinks.localID}</a>
								[<a title="alternative site" rel="external" href="http://www.legumes-online.net/ildis/aweb/redir/${taxon.taxonExternalLinks.localID}.htm">mirror</a>]
							</c:when>
							<c:otherwise>
								${taxon.taxonExternalLinks.localID}
							</c:otherwise>
						</c:choose>)
						</c:if>
					</p>
				</c:if>

			</section>
		</div>

		</small>
		</li>
    </p>

		</ul>
		</h2>
		</tags:pageheader>
<section id="printHeader" class="hide print">
<h2 id="page-title">
	<i class="glyphicons-icon leaf"><!--  --></i> <em>${taxon.scientificName}</em>&#160;<span>${taxon.scientificNameAuthorship}</span>
	<ul class="unstyled">
		<li>
			<small>
				<span id="protologue">${taxon.namePublishedInString}</span>
				<c:if test="${not empty taxon.nomenclaturalStatus}">&#160;<span ><spring:message code="nomenclatural_status_abbreviation_${taxon.nomenclaturalStatus}" text="${taxon.nomenclaturalStatus}"/></span></c:if>
			</small>
		</li>
		<li>
			<c:set var="preferred" value="${em:getPreferred(taxon)}" />
			<c:if test="${not empty preferred}">
				<small>${preferred.vernacularName}</small>
			</c:if>
		</li>
	</ul>
</h2>
</section>

<div class="container">
<c:forEach var="child" items="${em:sort(taxon.childNameUsages)}" end="0">
	<spring:message code="children" var="childrenHeader"/><spring:message code="${child.taxonRank}" var="childrenRank" />
</c:forEach>
<c:set var="taxonomicStatusReference" value="${em:taxonomicStatusReference(taxon)}" />
<c:set var="bibliography" value="${em:bibliography(taxon)}" />
<c:set var="provenance" value="${em:provenance(taxon)}" />
<header class="subhead clearfix" id="overview">
	<c:if test="${not empty taxon.images or not empty taxon.descriptions or not empty taxon.distribution or not empty taxon.childNameUsages or not empty taxon.synonymNameUsages or not empty bibliography.references or not empty taxonomicStatusReference.references}">
		<div class="subnav no-print">
			<ul class="nav nav-pills">
				<c:if test="${not empty taxon.images}">
					<li><a  href="#images"><spring:message code="images" /></a></li>
				</c:if>
				<c:if test="${not empty taxon.descriptions}">
					<li><a  href="#description"><spring:message code="description" /></a></li>
				</c:if>
				<c:if test="${not empty taxon.distribution}">
					<li><a  href="#distributionMap"><spring:message code="distributionMap" /></a></li>
				</c:if>
				<c:if test="${not empty taxon.childNameUsages}">
					<li><a  href="#children">${childrenHeader}${childrenRank}</a></li>
				</c:if>
				<c:if test="${not empty taxon.synonymNameUsages}">
					<li><a  href="#synonyms"><spring:message code="synonyms" /></a></li>
				</c:if>
				<c:if test="${not empty taxon.vernacularNames}">
					<c:set var="count" value="${em:getCount(taxon)}" />
					<c:if test= "${count == 1 || count== 0}">
						<c:set var="isTrue" value="${em:isPreferred(taxon)}" />
						<c:choose>
							<c:when test = "${isTrue == true}">
								<li><a  href="#preferredNames"><spring:message code="preferredNames" /></a></li>
							</c:when>
							<c:otherwise>
								<li><a  href="#localNames"><spring:message code="localNames" /></a></li>
							</c:otherwise>
						</c:choose>
					</c:if>
					<c:if test= "${count == 2}">
						<li><a  href="#preferredNames"><spring:message code="preferredNames" /></a></li>
						<li><a  href="#localNames"><spring:message code="localNames" /></a></li>
					</c:if>
				</c:if>
				<c:if test="${not empty taxonomicStatusReference.references}">
					<li><a  href="#taxonomicStatusReference"><spring:message code="taxonomicStatusReference" /></a></li>
				</c:if>
				<c:if test="${not empty bibliography.references}">
					<li><a  href="#bibliography"><spring:message code="bibliography" /></a></li>
				</c:if>
			</ul>
		</div>
	</c:if>
</header>
<tags:info />
<tags:error />
<c:if test="${not empty taxon.images}">
	<div class="container">
		<div id="images" data-toggle="modal-gallery" data-target="#modal-gallery" data-selector="a.slideshow">
			<div class="row">
				<div class="carousel slide span12" id="carousel">
					<div class="carousel-inner">
						<c:forEach var="i" items="${taxon.images}" step="6"	varStatus="status">
							<c:choose>
								<c:when test="${status.index == 0}">
									<c:set var="active" value="active" />
								</c:when>
								<c:otherwise>
									<c:set var="active" value="" />
								</c:otherwise>
							</c:choose>
							<div class="item ${active}">
								<ul class="thumbnails">
									<c:forEach var="image" items="${taxon.images}" begin="${status.index}" end="${status.index + 5}">
										<li>
											<c:choose>
												<c:when test="${not empty image.license}">
													<c:set var="imageLicense" value="${image.license}" />
												</c:when>
												<c:otherwise>
													<c:set var="imageLicense" value="All right reserved" />
												</c:otherwise>
											</c:choose>
											<c:url var="fullsize" value="/images/fullsize/${image.id}.${image.format}" />
											<c:url var="imageUrl" value="/image/${image.id}"/>
											<a class="slideshow" href="${fullsize}"
											   data-link="${imageUrl}" data-icon="icon-white icon-picture"
											   data-creator="${image.creator}" data-license="${imageLicense}" title="${em:stripXml(image.title)}">
												<c:url var="thumbnail" value="/images/thumbnails/${image.id}.${image.format}" />
												<img class="thumbnail" src="${thumbnail}" title="${em:stripXml(image.title)}" />
											</a>
										</li>
									</c:forEach>
								</ul>
							</div>
						</c:forEach>
					</div>
					<c:if test="${fn:length(taxon.images) > 6 }">
						<a data-slide="prev" href="#carousel" class="left carousel-control no-print">&#8249;</a>
						<a data-slide="next" href="#carousel" class="right carousel-control no-print">&#8250;</a>
					</c:if>
				</div>
			</div>
		</div>
	</div>
</c:if>

<script type="text/javascript">
	$(document).ready(function(){
		$('#clickMe').toggle(
				function(){
					$('#textBox').hide();
					$('#clickMe').text('Show details');
				},
				function(){
					$('#textBox').show();
					$('#clickMe').text('Hide details');
				});
	});

	$(document).ready(function() {
		$("a[href^='#']").click(function(event) {
			var targetId = $(this).attr('href');
			var section = $(targetId);
			section.css('scroll-margin', '2.5rem');
			event.preventDefault();
			smoothScroll(targetId);
			if(history.pushState) {
				history.pushState(null, null, targetId);
			}
			else {
				location.hash = targetId;
			}
		});
	});
	
	function smoothScroll(selector){
	 document.querySelector(selector).scrollIntoView({
		  behavior: "smooth", block: "start", inline: "nearest"
	 });
}
</script>

<div id="modal-gallery" class="modal modal-gallery hide fade modal-fullscreen modal-fullscreen-stretch no-print">
	<div class="modal-header">
		<a class="close" data-dismiss="modal">&#215;</a>
	</div>
	<div class="modal-body">
		<div class="modal-image"><!--  --></div>
		<div class="carousel-caption modal-title">
			<label id="clickMe" class="pull-right">Hide details</label>
			<ul id="textBox" class="unstyled" style="text-align:left;">
				<li>
					<a class="modal-download">
						<i class="modal-icon">&#160;</i>
						<span class="modal-caption">&#160;</span>
					</a>
				</li>
				<li>
					<em><spring:message code="image.creator"/></em>&#160;<span class="modal-creator">&#160;</span>
				</li>
				<li>
					<em><spring:message code="image.license"/></em>&#160;<span class="modal-license-icon">&#160;</span>
					<a class="btn btn-success modal-play modal-slideshow pull-right" data-slideshow="3000"><i class="icon-white icon-play"><!-- --></i></a>
				</li>
			</ul>
		</div>
	</div>
</div>
<br />

<div class="citation-text">
	<c:choose>
		<c:when test = "${not empty taxon.identifier and not empty taxon.scientificName}">
			<c:set var="author" value="${em:getAuthor(taxon.scientificNameAuthorship)}" />
			Cite taxon page as 'WFO (<jsp:expression>new SimpleDateFormat("yyyy").format(new Date())</jsp:expression>):  ${taxon.scientificName}&#160;<span>${author}</span>. Published on the Internet;<a href= "http://www.worldfloraonline.org/taxon/${taxon.identifier}">http://www.worldfloraonline.org/taxon/${taxon.identifier}</a>.
			Accessed on: <jsp:expression>new SimpleDateFormat("dd MMM yyyy").format(new Date())</jsp:expression>'
		</c:when>
		<c:otherwise>
			How to cite: 'WFO (<jsp:expression>new SimpleDateFormat("yyyy").format(new Date())</jsp:expression>): World Flora Online. Published on the Internet;<a href= "http://www.worldfloraonline.org/">http://www.worldfloraonline.org</a>.
			Accessed on: <jsp:expression>new SimpleDateFormat("dd MMM yyyy").format(new Date())</jsp:expression>'
		</c:otherwise>
	</c:choose>
</div>
<!--	commented taxonRemarks-->
<!--	<c:if test="${not empty taxon.taxonRemarks}">
<div id="taxonRemarks">
<spring:message code="notes" />${taxon.taxonRemarks}
</div>
</c:if> -->
<div class="row">
	<div class="span8">
			<c:if test="${not empty taxon.descriptions}">
				<section id="description" class="section-box">
					<c:forEach var="feature" items="${em:features()}">
						<c:set var="descriptions" value="${em:content(taxon,feature)}" />
						<c:if test="${not empty descriptions}">
							<div class="inner">
								<div id="${feature}" class="box">
									<spring:message code="${feature}" var="featureHeader"/>
									<h2>
										<span>${featureHeader}</span>
										<tags:feedbackLink selector="option:contains(${featureHeader})" dataName="${featureHeader}" />
									</h2>
									<c:forEach var="description" items="${descriptions}">
										<div class="description-with-citations">
											<p class="justified">${description.description}</p>
											<li style="list-style: none;  margin: 0px;  float: right; color: #888;">
												<c:set var="provenancekey" value="${em:provenancekey(provenance, description)}"/>
												<c:set var="provenancename" value="${em:provenancename(provenance, description)}"/>
												<spring:message code="citation.provenance" arguments="${provenancekey}|${provenancekey}|${provenancekey}|${provenancename}" argumentSeparator="|"/>
											</li>
											<ul class="citations">
												<c:if test="${not empty description.references}">
													<spring:message code="citation.source"/>

													<c:forEach var="reference" items="${description.references}">
														<li>
															<c:set var="citationKey" value="${em:citekey(bibliography, reference)}"/>
															<a href="#citation${citationKey}">${citationKey}</a>
														</li>
													</c:forEach>
													<spring:message code="]."/>&#160;
												</c:if>
											</ul>
										</div>
