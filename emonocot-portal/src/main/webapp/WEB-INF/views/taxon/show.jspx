								<li><a href="${keyUrl}" title="${key.title}">${key.title}</a></li>
							</c:forEach>
						</ul>
						<ul class="citations">
							<spring:message code="citation.providedby"/>
							<c:forEach var="provenanceHolder" items="${em:provenanceinitials(provenance,taxon.keys)}">
								<li>
									<a href="#${em:initialtokey(provenanceHolder)}">${provenanceHolder}</a>
								</li>
							</c:forEach>
						</ul>
					</section>
				</div>
			</div>
